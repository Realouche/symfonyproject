<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="User\UserBundle\Repository\UserRepository" name="User\UserBundle\Entity\User" table="fos_user_table">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="img" column="img" type="string" />
    <field name="adresse" column="adresse" type="string" />
    <field name="telephone" column="telephone" type="string" />
    <field name="dateBirth" column="dateBirth" type="date" />

  </entity>
</doctrine-mapping>
